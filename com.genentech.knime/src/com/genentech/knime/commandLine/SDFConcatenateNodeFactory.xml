<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../../icons/dna3.png" type="Manipulator">
    <name>SDF Concatenate</name>
    
    <shortDescription>
        SDF Concatenate
    </shortDescription>
    
    <fullDescription>
        <intro>Concatenates input of all input files in order of ports</intro>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="In-Port name 0">SDF at first port (required)</inPort>
        <inPort index="1" name="In-Port name 1">SDF at second port (optional)</inPort>
        <inPort index="2" name="In-Port name 2">SDF at third port (optional)</inPort>
        <inPort index="3" name="In-Port name 3">SDF at forth port (optional)</inPort>
        <!-- possibly more input ports here-->
        <outPort index="0" name="SDF">Concatenated SDF</outPort>
        <!-- possibly more output ports here-->
    </ports>
    <views>
        <view index="0" name="Command Error Output">Description of first view...</view>
    </views>  
</knimeNode>
